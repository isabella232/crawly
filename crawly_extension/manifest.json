{
  "name": "Crawly",
  "version": "1.0",
  "description": "crawls a site and fires all analytics events",
  "permissions": ["declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.4.1.min.js", "crawly.js"]
    }
  ],
  "manifest_version": 2
}
